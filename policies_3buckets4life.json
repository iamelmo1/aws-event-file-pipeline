{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadFromIngest",
      "Effect": "Allow",
      "Action": ["s3:GetObject"],
      "Resource": "arn:aws:s3:::angel-pipeline-ingest/*"
    },
    {
      "Sid": "WriteToProcessedAndQuarantine",
      "Effect": "Allow",
      "Action": ["s3:PutObject"],
      "Resource": [
        "arn:aws:s3:::angel-pipeline-processed/*",
        "arn:aws:s3:::angel-pipeline-quara/*"
      ]
    }
  ]
}
